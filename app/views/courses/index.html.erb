<h1>Courses index</h1>
<p><%= link_to 'New Course', new_course_path %></p>
<% @courses.each do |i| %>
 <h2><%= i.title %></h2>
 <h3><%= i.subject %></h3>
 <p><%= i.code %></p>
 <%= link_to 'Show Course', course_path(i) %>
 <%= link_to 'Edit Course', edit_course_path(i) %>
 <%= link_to 'Destroy Course', i, method: :delete %>
<% end %>
 <% byebug %>